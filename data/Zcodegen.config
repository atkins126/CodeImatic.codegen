<?xml version="1.0" encoding="utf-8" ?>
<config>
    <plugins>
        <SysTags>
          <filename>SysTags.dll</filename>
        </SysTags>
        <Markdown>
           <filename>Markdown.dll</filename>
           <properties>
              <outputextension>html</outputextension>
              <sourceextension>md</sourceextension>
           </properties>   
        </Markdown>
        <WebServer>
          <filename>WebServer.dll</filename>
          <properties>
              <DefaultDocument>index.html</DefaultDocument>
              <Server>127.0.0.1</Server>
              <Port>8080</Port>
              <UseSSL>true</UseSSL>
              <SSLPassword>aaaa</SSLPassword>
              <SSLKeyFile>sample.key</SSLKeyFile>
              <SSLCertFile>sample.crt</SSLCertFile>
              <SSLRootCertFile>sampleRoot.pem</SSLRootCertFile>
          </properties>
        </WebServer>                                 
        <SystemExt>
           <filename>SystemExt.dll</filename>
        </SystemExt>
        <StaticWeb>
           <filename>StaticWeb.dll</filename>
        </StaticWeb>
    </plugins>
</config>